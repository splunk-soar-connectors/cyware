            "primary": true,
            "required": true
        },
        "description":{
          "description": "Description of a incident",
            "data_type": "string",
            "primary": true,
            "required": true
        },
        "file":{
          "description": "Vault IDs of file to attach.",
            "data_type": "string",
            "contains": ["vault id"],
